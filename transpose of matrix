#include<stdio.h>
void main()
{
        int m,n,i,j,e=0,o=0;
        printf("Enter the Row and Column of Array: ");
        scanf("%d%d",&m,&n);
        int arr[m][n];
        for(i=0;i<m;i++){
               for(j=0;j<n;j++){
                        printf("Enter the elements of Array: ");
                        scanf("%d",&arr[i][j]);
                }
        }
        printf("Matrix is:\n");
        for(i=0;i<m;i++){
                for(j=0;j<n;j++){
                        printf("%4d",arr[i][j]);
                }
                printf("\n");
        }
        printf("Transpose of matrix is:\n");
        for(i=0;i<m;i++){
                for(j=0;j<n;j++){
                        printf("%4d",arr[j][i]);
                }
                printf("\n");
        }
}
