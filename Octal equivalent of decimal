#include<stdio.h>
void main()
{
        int a[20],i,m,n,r;
        printf("Enter the decimal number \n");
        scanf("%d",&n);
        m=n;
        for(i=0;n>0;i++)
        {
                r=n%8;
                a[i]=r;
                n=n/8;
        }
        printf("Octal equivalant of %d is ",m);
        for (i--;i>=0;i--){
                printf("%d",a[i]);
        }
}
