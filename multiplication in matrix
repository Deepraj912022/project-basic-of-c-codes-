#include<stdio.h>
void main() {
    int m, n, i, j, k;
    printf("Enter the number of rows and columns for both Matrices: \n");
    scanf("%d %d", &m, &n);
    int arr1[m][n], arr2[m][n], result[m][n];
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            result[i][j] = 0;
        }
    }
    printf("Enter elements of the Matrix A:\n");
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &arr1[i][j]);
        }
    }
    printf("Enter elements of the Matrix B:\n");
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &arr2[i][j]);
        }
    }
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            for (k = 0; k < n; k++) {
                result[i][j] += arr1[i][k] * arr2[k][j];
            }
        }
    }
    printf("Resultant matrix after multiplicationÂ is\n");
